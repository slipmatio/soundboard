{
  "extends": [
    "config:base",
    "group:all",
    "schedule:weekly"
  ],
  "labels": [
    "dependencies"
  ],
  "baseBranches": [
    "main"
  ],
  "packageRules": [
    {
      "updateTypes": [
        "patch"
      ],
      "enabled": false
    },
    {
      "matchPackagePatterns": [
        "*"
      ],
      "rangeStrategy": "replace"
    }
  ],
  "timezone": "Europe/Helsinki",
  "dependencyDashboard": true
}
